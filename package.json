{
  "name": "yesaind",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run build && scripts/serve.sh",
    "dev:hmr": "scripts/dev.sh",
    "build": "vite build",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "typecheck": "npm run cf-typegen && tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "migrate:local": "wrangler d1 migrations apply collabboard-db --local",
    "migrate:remote": "wrangler d1 migrations apply collabboard-db --remote",
    "migrate": "npm run migrate:local && npm run migrate:remote",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "health": "node scripts/health.js",
    "prepare": "git config core.hooksPath scripts/hooks",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "traces": "scripts/traces.sh",
    "traces:remote": "TRACES_ENV=remote scripts/traces.sh",
    "update-deps": "npx npm-check-updates -u --reject 'vite,@vitejs/plugin-react' && npm ci",
    "eval": "npx tsx scripts/prompt-eval.ts",
    "eval:narrative": "EVAL_SKIP_LAYOUT=1 npx tsx scripts/prompt-eval.ts",
    "eval:layout": "EVAL_SKIP_NARRATIVE=1 npx tsx scripts/prompt-eval.ts",
    "eval:compare": "npx tsx scripts/prompt-compare.ts"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.46",
    "@ai-sdk/openai": "^3.0.30",
    "@ai-sdk/react": "^3.0.99",
    "@cloudflare/ai-chat": "^0.1.3",
    "@simplewebauthn/browser": "^13.2.2",
    "@simplewebauthn/server": "^13.2.2",
    "agents": "^0.5.1",
    "ai": "^6.0.97",
    "hono": "^4.12.1",
    "konva": "^10.2.0",
    "langfuse": "^3.38.6",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-konva": "^19.2.2",
    "workers-ai-provider": "^3.1.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.3.0",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@types/ws": "^8.18.1",
    "@vitejs/plugin-react": "^5.0.0",
    "concurrently": "^9.2.1",
    "typescript": "^5.9.3",
    "vite": "^7.0.0",
    "wrangler": "^4.67.0"
  },
  "overrides": {
    "minimatch": ">=10.2.1"
  }
}
