{
  "name": "collabboard",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm run build && scripts/serve.sh",
    "dev:hmr": "scripts/dev.sh",
    "build": "vite build",
    "deploy": "npm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "typecheck": "npm run cf-typegen && tsc --noEmit",
    "lint": "eslint src/",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "migrate:local": "wrangler d1 migrations apply collabboard-db --local",
    "migrate:remote": "wrangler d1 migrations apply collabboard-db --remote",
    "migrate": "npm run migrate:local && npm run migrate:remote",
    "test:e2e": "npx playwright test",
    "test:e2e:ui": "npx playwright test --ui",
    "demo": "scripts/demo.sh",
    "demo:e2e": "scripts/demo.sh e2e",
    "health": "node scripts/health.js"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.45",
    "@cloudflare/ai-chat": "^0.1.2",
    "agents": "^0.5.0",
    "ai": "^6.0.91",
    "hono": "^4.11.10",
    "konva": "^10.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-konva": "^19.2.2",
    "workers-ai-provider": "^3.1.1",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/node": "^25.2.3",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.56.0",
    "@typescript-eslint/parser": "^8.56.0",
    "@vitejs/plugin-react": "^4.3.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "typescript": "^5.7.0",
    "vite": "^6.1.0",
    "wrangler": "^4.66.0"
  }
}
